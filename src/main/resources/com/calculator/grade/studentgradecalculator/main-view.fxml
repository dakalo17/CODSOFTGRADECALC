<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<ScrollPane fx:id="spMain" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.calculator.grade.studentgradecalculator.controllers.MainController">
    <HBox prefHeight="200.0">
        <VBox fx:id="vbMain">
            <HBox fx:id="hbButtons" alignment="BOTTOM_LEFT">
                <Button fx:id="btnAddEntry" mnemonicParsing="false" onAction="#onAddEntry" text="Add entry" />
                <Button fx:id="btnCalculate" alignment="CENTER" mnemonicParsing="false" onAction="#onCalculate" text="Calculate" />
             <opaqueInsets>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
             </opaqueInsets>
             <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
             </padding>
            </HBox>
          <opaqueInsets>
             <Insets />
          </opaqueInsets>
        </VBox>
      <TableView fx:id="tbvResult">
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"  />
         </columnResizePolicy></TableView>
    </HBox>
</ScrollPane>
